import java.util.Scanner;

public class Uri1143 {
	
	public static void main(String args[]) {
		
		
		Scanner teclado;
		teclado = new Scanner(System.in);
		int linhas;
		
		linhas = teclado.nextInt();
		
		for(int i=0;i<linhas;i++) {
			for(int j=0;j<3;j++) {
				System.out.print(linhas);
			}
		}
		
		teclado.close();
		
		
	}

}
