import java.util.Scanner;

public class Uri1143 {
	
	public static void main(String args[]) {
		
		
		Scanner teclado;
		teclado = new Scanner(System.in);
		int linhas;
		int j;
		
		linhas = teclado.nextInt();
		
		for(int i=0;i<linhas;linhas++) {
		
				System.out.print(i + " " + i*i + " " + i * i * i);
			
			
		}
		
		teclado.close();
		
		
	}

}
